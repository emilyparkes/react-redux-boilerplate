{
  "name": "react-redux-boilerplate",
  "version": "0.1.0",
  "description": "react and redux boilerplate",
  "author": "emily Parkes",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config ./client/webpack.config.js",
    "dev": "nodemon --watch server & yarn build --watch",
    "knex": "knex --knexfile ./server/db/knexfile.js",
    "seed": "yarn knex seed:run",
    "postinstall": "yarn build && yarn knex migrate:latest",
    "test": "jest test/client test/server",
    "test:watch": "yarn test --watchAll",
    "test:assert": "yarn test && yarn assert",
    "start": "node server"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "dotenv": "^5.0.1",
    "express": "^4.15.3",
    "knex": "^2.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "core-js": "^2.5.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "input-moment": "^0.4.0",
    "jest": "^21.1.0",
    "jsdom": "^11.2.0",
    "nodemon": "^1.11.0",
    "pre-commit": "^1.2.2",
    "raf": "^3.4.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.2",
    "redux-mock-store": "^1.3.0",
    "redux-thunk": "^2.1.0",
    "sqlite3": "^3.1.9",
    "superagent": "^3.8.1",
    "supertest": "^3.0.0",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1"
  }
}
